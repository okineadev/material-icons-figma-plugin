		"bun-types": "^1.1.45",
		"husky": "^9.1.7",
		"lint-staged": "^15.4.1",
		"material-icon-theme": "^5.17.0",
		"plugma": "^1.2.7",
		"rimraf": "^6.0.1",
		"sass-embedded": "^1.83.4",
		"typescript": "^5.7.3",
		"vite": "^6.0.7"
	},
	"trustedDependencies": ["@biomejs/biome"]
